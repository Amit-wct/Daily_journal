grant commands in db

GRANT USAGE ON SCHEMA public TO domains_role ;

grant SELECT, INSERT,  update,  DELETE, REFERENCES, TRIGGER on all tables in schema public to domains_role;

GRANT USAGE, SELECT ON ALL SEQUENCES IN SCHEMA public TO domains_role ;
